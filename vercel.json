{
    "framework": "nextjs",
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "cleanUrls": true,
    "trailingSlash": false,
    "crons": [
        {
            "path": "/api/cron/generate",
            "schedule": "0 * * * *"
        },
        {
            "path": "/api/cron/publish",
            "schedule": "0 */13 * * *"
        }
    ]
}